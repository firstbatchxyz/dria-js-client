{
  "name": "dria",
  "version": "0.0.1",
  "author": "FirstBatch Team <dev@firstbatch.xyz>",
  "contributors": [
    "Erhan Tezcan <erhan@firstbatch.xyz> (https://github.com/erhant)"
  ],
  "scripts": {
    "cli": "bun run ./src/bin/index.ts",
    "check": "tsc --noEmit && echo \"All good.\"",
    "format": "prettier --check '**/*.ts'",
    "lint": "eslint '**/*.ts' && echo 'All good.'",
    "t": "bun test",
    "proto:code:insert": "npx pbjs ./proto/insert.proto -w commonjs -t static-module -o ./proto/insert.js",
    "proto:type:insert": "npx pbts ./proto/insert.js -o ./proto/insert.d.ts"
  },
  "license": "Apache-2.0",
  "type": "module",
  "module": "index.ts",
  "dependencies": {
    "axios": "^1.6.5",
    "yargs": "^17.7.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bun": "^1.0.4",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.4",
    "protobufjs-cli": "^1.1.2",
    "typescript": "^5.0.0"
  },
  "prettier": {
    "printWidth": 120
  },
  "keywords": [
    "dria",
    "blockchain",
    "firstbatch",
    "hnsw",
    "ai",
    "vector",
    "vectordb",
    "rag"
  ]
}
